<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.Evermind.ui.dashboard.ui.main.NoteEditorFragmentJavaFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_note_creator" />

    <action
        android:id="@+id/action_nav_grid_to_nav_note"
        app:destination="@id/nav_note"
        app:enterAnim="@anim/fade_in"
        app:exitAnim="@anim/fade_out_button" />

</navigation>